<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    tools:context="com.carltaylordev.recordlisterandroidclient.ListingActivity">


    <EditText
        android:id="@+id/listing_activity_edit_text_artist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Artist"
        />

    <EditText
        android:id="@+id/listing_activity_edit_text_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Title"
        />


    <EditText
        android:id="@+id/listing_activity_edit_text_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Label"
        />

    <Spinner
        android:id="@+id/listing_activity_spinner_cats"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></Spinner>







</LinearLayout>
